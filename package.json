{
    "name": "atomx",
    "displayName": "Atomx",
    "description": "Atom tools for VSCode",
    "version": "0.0.3",
    "publisher": "johnnyyao",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage: atom"
    ],
    "main": "./extension",
    "contributes": {
        "languages": [{
            "id": "atom",
            "aliases": ["Atom", "atom"],
            "extensions": [".atom"]
        }],
        "grammars": [{
            "language": "atom",
            "scopeName": "source.vue",
            "path": "./syntaxes/vue.tmLanguage",
            "embeddedLanguages": {
                "text.html.basic": "html",
                "source.css": "css",
                "source.css.less": "less",
                "source.stylus": "stylus",
                "source.js": "javascript"
            }
        }],
        "snippets": [
            {
                "language": "atom",
                "path": "./snippets/atom.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Sheetaa/atomx.git"
    },
    "keywords": [
        "Atom",
        "atom",
        "VS Code",
        "vs code",
        "vue"
    ],
    "author": "Johnny <wuhayao@gmail.com>"
}
